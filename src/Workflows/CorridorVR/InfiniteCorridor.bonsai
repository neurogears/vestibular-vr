        <TranslationY>-1</TranslationY>
        <TranslationZ>100</TranslationZ>
        <RotationZ>0</RotationZ>
        <RotationY>90</RotationY>
        <RotationX>90</RotationX>
        <ScaleX>1000</ScaleX>
        <ScaleY>2</ScaleY>
        <ScaleZ>1</ScaleZ>
