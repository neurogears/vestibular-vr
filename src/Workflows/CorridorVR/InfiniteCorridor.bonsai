          <Y>0.8</Y>
          <Z>0.8</Z>
        </Specular>
        <SpecularExponent>10</SpecularExponent>
        <MeshName>Plane</MeshName>
      </Expression>
      <Expression xsi:type="rx:BehaviorSubject">
        <Name>RenderFrames</Name>
      </Expression>
      <Expression xsi:type="SubscribeSubject">
        <Name>Draw_Stimulus</Name>
